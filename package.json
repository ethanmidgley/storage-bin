{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:remix\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "tailwindcss -w -i ./styles/tailwind.css -o ./app/tailwind.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@emotion/react": "11",
    "@emotion/server": "11",
    "@emotion/styled": "11",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@remix-run/serve": "^1.7.4",
    "framer-motion": "6",
    "isbot": "^3.5.4",
    "prisma": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.5.0",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
